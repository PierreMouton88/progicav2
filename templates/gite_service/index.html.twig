{% extends 'base.html.twig' %}

{% block title %}GiteService index{% endblock %}

{% block body %}
    <h1>GiteService index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Price</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for gite_service in gite_services %}
            <tr>
                <td>{{ gite_service.id }}</td>
                <td>{{ gite_service.price }}</td>
                <td>
                    <a href="{{ path('app_gite_service_show', {'id': gite_service.id}) }}">show</a>
                    <a href="{{ path('app_gite_service_edit', {'id': gite_service.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_gite_service_new') }}">Create new</a>
{% endblock %}
